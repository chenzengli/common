<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="56dp">

    <View
        android:id="@+id/view_blank"
        android:layout_width="match_parent"
        android:layout_height="@dimen/interval_6dp"
        android:background="@color/white_dd" />

    <ImageView
        android:id="@+id/cb_cart_shop"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/view_blank"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:scaleType="center"
        android:src="@drawable/ic_radio_unselected"
        tools:ignore="ContentDescription" />

    <!--<ImageView-->
    <!--android:id="@+id/iv_shop_logo"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_centerVertical="true"-->
    <!--android:layout_below="@id/view_blank"-->
    <!--android:layout_toRightOf="@+id/check_box_group"-->
    <!--android:src="@drawable/ic_small_shop_icon"-->
    <!--tools:ignore="ContentDescription" />-->

    <TextView
        android:id="@+id/tv_shop_name"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/view_blank"
        android:layout_toEndOf="@id/cb_cart_shop"
        android:layout_toRightOf="@id/cb_cart_shop"
        android:drawableEnd="@drawable/ic_chevron_right_black_333"
        android:drawablePadding="10dp"
        android:drawableRight="@drawable/ic_chevron_right_black_333"
        android:gravity="center_vertical"
        android:text=""
        android:textColor="@color/black_333"
        android:textSize="@dimen/text_size_14sp" />


</RelativeLayout>